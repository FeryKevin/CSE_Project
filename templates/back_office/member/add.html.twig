{% extends 'back_office/base.html.twig' %}

{% block title %}Ajouter un membre {% endblock %}

{% block body %}
    <div class='update-about'>

        <h3 class='hAbout'>Ajouter un membre:</h3>

        <div class='member-form'>
            {{ form_start(form) }}
                {{ form_row(form.name) }}
                {{ form_row(form.firstname) }}
                {{ form_row(form.image) }}
                {{ form_widget(form.submit) }}
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}